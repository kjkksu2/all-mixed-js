{
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-empty": "off",
    "no-constant-condition": "off",
    "no-undef": "off"
  }
}
